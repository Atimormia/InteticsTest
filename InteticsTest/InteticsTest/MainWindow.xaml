<Window x:Class="InteticsTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InteticsTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="newOrder" Content="New order" HorizontalAlignment="Left" Margin="10,94,0,0" VerticalAlignment="Top" Width="497" Height="50" Click="newOrder_Click"/>
        <Button x:Name="clientCard" Content="Client card" HorizontalAlignment="Left" Margin="10,149,0,0" VerticalAlignment="Top" Width="497" Height="50" Click="clientCard_Click"/>
        <Button x:Name="OrdersByCar" Content="Orders by car" HorizontalAlignment="Left" Margin="10,204,0,0" VerticalAlignment="Top" Width="497" Height="50"/>
        <Button x:Name="exit" Content="Exit" HorizontalAlignment="Left" Margin="10,259,0,0" VerticalAlignment="Top" Width="497" Height="50" Click="exit_Click"/>
        <Label x:Name="label" Content="Service Station" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="497" Height="46"/>
        <Label x:Name="label1" Content="Administrator" HorizontalAlignment="Left" Margin="10,61,0,0" VerticalAlignment="Top" Width="497" Height="28"/>
    </Grid>
</Window>
